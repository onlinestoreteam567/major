{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@svg/*": ["src/assets/svg/*"],
      "@components/*": ["src/components/*"],
      "@UI/*": ["src/components/UI/*"],
      "@config/*": ["src/config/*"],
      "@features/*": ["src/features/*"],
      "@hooks/*": ["src/hooks/*"],
      "@pages/*": ["src/pages/*"],
      "@router/*": ["src/router/*"],
      "@services/*": ["src/services/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],
      "@mixins/*": ["src/styles/mixins/*"],
      "@../*": ["src/*"]
    }
  }
}
